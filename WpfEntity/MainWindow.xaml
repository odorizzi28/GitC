<Window x:Class="WpfEntity.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfEntity"
        xmlns:y="clr-namespace:WpfEntity.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <y:ucLogin x:Name="ucLoginForm"
                   sucess="UcLogin_sucess"
                   fail="UcLogin_fail"></y:ucLogin>
        <y:ucGridView x:Name="ucGrid" Visibility="Hidden"/>
    </Grid>
</Window>
